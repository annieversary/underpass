{
  "name": "underpass",
  "version": "1.0.0",
  "description": "(wip name until we decide on something else)",
  "main": "index.js",
  "scripts": {
    "build": "esbuild frontend/index.ts --bundle --sourcemap --target=es2017 --outfile=public/index.js",
    "watch": "esbuild frontend/index.ts --bundle --sourcemap --target=es2017 --outfile=public/index.js --watch",
    "typecheck": "tsc -noEmit --lib es2022,DOM --target es2017 --moduleResolution node --skipLibCheck frontend/index.ts",
    "prod": "esbuild frontend/index.ts --bundle --minify --target=es2017 --outfile=public/index.js && cp frontend/index.html public"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "0.19.5",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.10.2",
    "@codemirror/commands": "^6.3.0",
    "@codemirror/language": "^6.9.2",
    "@codemirror/view": "^6.21.4",
    "@maplibre/maplibre-gl-geocoder": "^1.5.0",
    "@turf/length": "^6.5.0",
    "@types/geojson": "^7946.0.12",
    "events": "^3.3.0",
    "maplibre-gl": "^3.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rete": "^2.0.2",
    "rete-area-plugin": "^2.0.1",
    "rete-connection-plugin": "^2.0.0",
    "rete-context-menu-plugin": "^2.0.0",
    "rete-react-plugin": "^2.0.4",
    "rete-render-utils": "^2.0.1",
    "styled-components": "^6.1.0"
  }
}
