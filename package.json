{
  "name": "underpass",
  "version": "1.0.0",
  "description": "(wip name until we decide on something else)",
  "main": "index.js",
  "scripts": {
    "build": "esbuild frontend/index.ts --bundle --sourcemap --outfile=public/index.js",
    "watch": "esbuild frontend/index.ts --bundle --sourcemap --outfile=public/index.js --watch",
    "typecheck": "tsc -noEmit frontend/index.ts",
    "prod": "esbuild frontend/index.ts --bundle --minify --outfile=public/index.js && cp frontend/index.html public"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "0.19.5",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@maplibre/maplibre-gl-geocoder": "^1.5.0",
    "@turf/length": "^6.5.0",
    "events": "^3.3.0",
    "maplibre-gl": "^3.5.2",
    "tsc": "^2.0.4"
  }
}
